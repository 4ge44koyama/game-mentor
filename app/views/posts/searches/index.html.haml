= render partial: "/posts/site-image"
.contents
  - @posts.each do |post|
    = render partial: "/posts/post", locals: { post: post }
= paginate(@posts) if @posts.present?
.touroku
  - unless user_signed_in?
    .register-link
      = link_to "無料登録", new_user_registration_path, class: "register-link-btn"